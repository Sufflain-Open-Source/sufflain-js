{
    "type": "module",
    "name": "routify-starter",
    "private": true,
    "scripts": {
        "apimon": "nodemon --exec babel-node api/main.js",
        "dev": "run-p dev:* apimon",
        "dev:routify": "routify",
        "dev:vite": "vite --host 0.0.0.0",
        "preview": "vite preview",
        "build": "rm -rf dist && routify -b && vite build && javascript-obfuscator --output dist dist"
    },
    "devDependencies": {
        "@roxi/routify": "^2.18.4",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.34",
        "autoprefixer": "^10.4.2",
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "javascript-obfuscator": "^4.0.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "rimraf": "^3.0.2",
        "sirv-cli": "^2.0.2",
        "svelte": "^3.45.0",
        "svelte-preprocess": "^4.10.1",
        "vite": "^2.7.10"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.17.3",
        "node-fetch": "^3.2.3"
    }
}
